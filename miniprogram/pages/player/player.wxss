/* pages/player/player.wxss */

.player-container,
.player-info {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
}

.player-bkg {
  position: absolute;
  width: 100%;
  height: 100%;
  filter: blur(40rpx);
  opacity: 0.5;
}

.player-mask {
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: -1;
  background: #696969;
}

.player-disc {
  width: 592rpx;
  height: 592rpx;
  background: url(data:image/png;base64,) no-repeat center/cover;
  position: absolute;
  left: 50%;
  transform: translate(-50%, 140rpx);
}

.player-disc::after {
  content: '';
  width: 192rpx;
  height: 274rpx;
  position: absolute;
  top: -150rpx;
  left: 266rpx;
  background: url(data:image/png;base64,) no-repeat center/cover;
  transform: rotate(-15deg);
  transform-origin: 24rpx 10rpx;
  transition: transform 0.5s ease;
}

.player-disc_img {
  width: 368rpx;
  height: 368rpx;
  border-radius: 50%;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-top: -184rpx;
  margin-left: -184rpx;
}

.player-progress {
  width: 90%;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 24%;
}

.player-control {
  position: absolute;
  left: 50%;
  bottom: 8%;
  transform: translateX(-50%);
  display: flex;
  align-items: center;
}

.player-control .iconfont {
  color: #fefefe;
}

.icon-shangyishou,
.icon-xiayishou {
  font-size: 100rpx;
}

.icon-bofang,
.icon-zanting {
  font-size: 120rpx;
  margin: 0 60rpx;
}

.rotation {
  animation: ratation 12s linear infinite;
  -webkit-animation: ratation 12s linear infinite;
}

/*  */
.rotation-paused {
  animation-play-state: paused;
  -webkit-animation-play-state: paused;
}

@keyframes ratation {
  from {
    transform: rotate(0deg);
  }

  to {
    transform: rotate(360deg);
  }
}

.play.player-disc::after {
  transform: rotate(0deg);
}